{
    "python.defaultInterpreterPath": "[pipenv path]\\Scripts",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true,
        "**/__pycache__": true
    },
    "task.allowAutomaticTasks": "on",

    "cSpell.words": [
        "myset",
        "startime"
    ]
}